{
  "name": "halberdier",
  "version": "1.0.3",
  "description": "A local (offline) password manager for Mac",
  "engines": {
    "node": "^16.13.1",
    "npm": "^8.1.2"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "rm -rf dist && webpack watch",
    "package": "rm -rf Halberdier-darwin-x64 && electron-packager . Halberdier --icon icons/halberdier.icns --electron-version 16.0.6 && open Halberdier-darwin-x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-n-d-r-3-w/halberdier"
  },
  "keywords": [
    "local",
    "offline",
    "password",
    "manager",
    "mac"
  ],
  "author": "andr3w",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "babel-loader": "^8.2.3",
    "electron": "16.0.6",
    "electron-packager": "^15.4.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "material-ui": "^1.0.0-beta.33",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
