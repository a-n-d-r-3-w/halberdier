{
  "name": "halberdier",
  "version": "1.0.3",
  "description": "A local (offline) password manager for Mac",
  "engines": {
    "node": "^16.13.1",
    "npm": "^8.1.2"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "rm -rf dist && webpack watch",
    "package": "rm -rf Halberdier-darwin-x64 && electron-packager . Halberdier2022 --icon icons/halberdier.icns --electron-version 16.0.6 && open Halberdier2022-darwin-x64",
    "create-installer": "electron-installer-dmg --out=Halberdier2022-darwin-x64 Halberdier2022-darwin-x64/Halberdier2022.app Halberdier2022 && open Halberdier2022-darwin-x64"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-n-d-r-3-w/halberdier"
  },
  "keywords": [
    "local",
    "offline",
    "password",
    "manager",
    "mac"
  ],
  "author": "andr3w",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "appdmg": "^0.6.2",
    "babel-loader": "^8.2.3",
    "electron": "16.0.6",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^15.4.0",
    "electron-reloader": "^1.2.1",
    "eslint": "^8.6.0",
    "eslint-plugin-react": "^7.28.0",
    "prettier": "^2.5.1",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
